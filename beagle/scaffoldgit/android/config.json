{
  "dockerImageBuilder": "cimg/base:stable-20.04",
  "inputs": [
    {
      "cache": {
        "active": true,
        "newLabel": "Type new value.",
        "qty": 4
      },
      "label": "Your email: ",
      "name": "email",
      "type": "text"
    },
    {
      "label": "Project name: ",
      "name": "project_name",
      "type": "text"
    },
    {
      "cache": {
        "active": true,
        "newLabel": "Type new value.",
        "qty": 4
      },
      "label": "Package name: (ex: com.example)",
      "name": "package_name",
      "type": "text"
    },
    {
      "cache": {
        "active": true,
        "newLabel": "Type new value.",
        "qty": 4
      },
      "label": "Beagle version: (ex: 1.0.0)",
      "name": "beagle_version",
      "type": "text"
    },
    {
      "cache": {
        "active": true,
        "newLabel": "Type new value.",
        "qty": 4
      },
      "label": "BFF url: (ex: https://URL)",
      "name": "beagle_url",
      "type": "text"
    },
    {
      "cache": {
        "active": true,
        "newLabel": "Type new value.",
        "qty": 4
      },
      "label": "Min version SDK android: (ex: 21)",
      "name": "min_sdk",
      "type": "text"
    },
    {
      "cache": {
        "active": true,
        "newLabel": "Type new value.",
        "qty": 4
      },
      "label": "Target version SDK: (ex: 29)",
      "name": "target_sdk",
      "type": "text"
    },
    {
      "name": "username",
      "type": "CREDENTIAL_GITHUB_USERNAME"
    },
    {
      "name": "token",
      "type": "CREDENTIAL_GITHUB_TOKEN"
    }
  ]
}
